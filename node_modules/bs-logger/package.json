{
  "name": "bs-logger",
  "version": "0.2.6",
  "description": "Bare simple logger for NodeJS",
  "keywords": [
    "bare simple logger",
    "simple logger",
    "simple",
    "logger",
    "typescript",
    "lib",
    "library"
  ],
  "repository": "git+https://github.com/huafu/bs-logger.git",
  "license": "MIT",
  "author": "Huafu Gandon <huafu.gandon@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf dist coverage",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "lint": "tslint --project tsconfig.json --format stylish",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "postcommit": "git reset",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "test": "jest --coverage",
    "posttest": "npm run typecheck && npm run lint",
    "test:watch": "jest --watch",
    "typecheck": "tsc -p . --noEmit",
    "preversion": "npm test",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "tslint --fix",
        "git add"
      ]
    }
  },
  "dependencies": {
    "fast-json-stable-stringify": "2.x"
  },
  "devDependencies": {
    "@commitlint/cli": "7.x",
    "@commitlint/config-conventional": "7.x",
    "@types/jest": "23.x",
    "@types/node": "10.x",
    "conventional-changelog-cli": "2.x",
    "husky": "0.x",
    "jest": "23.x",
    "lint-staged": "7.x",
    "prettier": "1.x",
    "rimraf": "2.x",
    "ts-jest": "23.x",
    "tslint": "5.x",
    "tslint-config-prettier": "1.x",
    "tslint-plugin-prettier": "2.x",
    "typescript": "3.x"
  },
  "engines": {
    "node": ">= 6"
  },
  "_from": "bs-logger@0.2.6",
  "_integrity": "sha512-pd8DCoxmbgc7hyPKOvxtqNcjYoOsABPQdcCUjGp3d42VR2CX1ORhk2A87oqqu5R1kk+76nsxZupkmyd+MVtCog==",
  "_resolved": "https://registry.npmjs.org/bs-logger/-/bs-logger-0.2.6.tgz"
}