  },
  "xo": {
    "prettier": true,
    "rules": {
      "prefer-destructuring": 0,
      "prefer-arrow-callback": 0,
      "no-var": 0,
      "object-shorthand": 0,
      "unicorn/no-for-loop": 0,
      "unicorn/prefer-string-slice": 0,
      "unicorn/string-content": 0
    },
    "space": true
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcovonly",
      "text"
    ]
