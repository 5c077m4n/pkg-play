  "devDependencies": {
    "ava": "~0.25.0",
    "codecov": "^3.6.5",
    "mockery": "^2.1.0",
    "np": "^2.20.1",
    "npm-check": "^5.9.2",
    "nyc": "^11.6.0",
    "pify": "^3.0.0",
    "string-to-stream": "^1.1.0",
    "through": "^2.3.8",
